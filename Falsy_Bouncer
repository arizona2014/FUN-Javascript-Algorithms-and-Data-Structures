Javascript

function bouncer(arr) {
  // Don't show a false ID to this bouncer.
  var arrLun = arr.length;
  for(i=0;i<arrLun;i++){
    if(arr[i] === false || arr[i] === null || arr[i] === 0 || arr[i] === "" || arr[i] === undefined || typeof arr[i] !== "string" && isNaN(arr[i])){
      arr.splice(i,1);
      arrLun = arr.length;
      i=-1;
      continue;
    }
  }
  
  return arr;
}
bouncer([false, null, 0, NaN, undefined, ""]);

